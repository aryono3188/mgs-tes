<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>aksi_mahasiswa</servlet-name>
        <servlet-class>control_aksi.aksi_mahasiswa</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>aksi_fakultas</servlet-name>
        <servlet-class>control_aksi.aksi_fakultas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>aksi_jurusan</servlet-name>
        <servlet-class>control_aksi.aksi_jurusan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>aksi_mata_kuliah</servlet-name>
        <servlet-class>control_aksi.aksi_mata_kuliah</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>aksi_mahasiswa</servlet-name>
        <url-pattern>/aksi_mahasiswa</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>aksi_fakultas</servlet-name>
        <url-pattern>/aksi_fakultas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>aksi_jurusan</servlet-name>
        <url-pattern>/aksi_jurusan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>aksi_mata_kuliah</servlet-name>
        <url-pattern>/aksi_mata_kuliah</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
